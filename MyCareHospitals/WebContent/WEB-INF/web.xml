<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>generate</servlet-name>
    <servlet-class>com.niit.mycare.dataconfig.GenerateTables</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>generate</servlet-name>
    <url-pattern>/generate</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>addDoctor</servlet-name>
    <servlet-class>com.niit.mycare.doctorservices.AddDoctor</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addDoctor</servlet-name>
    <url-pattern>/addDoctor</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>addPatient</servlet-name>
    <servlet-class>com.niit.mycare.patientservices.AddPatient</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addPatient</servlet-name>
    <url-pattern>/addPatient</url-pattern>
  </servlet-mapping>
</web-app>